@using Syncfusion.EJ2
@using Syncfusion.EJ2.Schedule

@{
    var dataManager = new DataManager() { 
        Url = "https://localhost:44360/api/",
        Adaptor = "ODataV4Adaptor",
        CrossDomain = true 
    };
}

